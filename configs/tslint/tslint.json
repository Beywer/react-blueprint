{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ],
    "member-ordering": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ]
  },
  "rulesDirectory": []
}
