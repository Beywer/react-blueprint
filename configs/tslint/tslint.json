{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ],
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "max-line-length": false,
    "semicolon": false,
    "interface-name": false
  },
  "rulesDirectory": []
}
