{
  "name": "react-blueprint",
  "version": "2.0.0",
  "description": "My react app skeleton",
  "scripts": {
    "start": "cross-env NODE_ENV='development' webpack serve --config configs/webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV='production' webpack --config configs/webpack/webpack.prod.js",
    "test": "jest -c configs/jest/jest.config.js --watchAll",
    "lint": "npm-run-all eslint stylelint --continue-on-error",
    "lint:fix": "npm-run-all eslint:fix stylelint:fix prettier --continue-on-error",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier": "prettier src/**/*.{ts,tsx,scss} configs/**/*.js --write"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.16",
    "eslint": "^8.8.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "redux-devtools-extension": "^2.13.9",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3"
  },
  "browserslist": [
    "ie >= 11"
  ]
}
