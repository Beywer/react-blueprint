{
  "name": "react-blueprint",
  "version": "2.0.0",
  "description": "My react app skeleton",
  "scripts": {
    "start": "cross-env NODE_ENV='development' webpack-dev-server --config configs/webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV='production' webpack --config configs/webpack/webpack.prod.js",
    "lint": "npm run tslint && npm run stylelint",
    "lint:fix": "npm run tslint:fix npm run stylelint:fix",
    "tslint": "tslint -c configs/tslint/tslint.json src/**/*.ts src/**/*.tsx",
    "tslint:fix": "npm run tslint -- --fix",
    "stylelint": "stylelint src/**/*.css",
    "stylelint:fix": "npm run stylelint -- --fix"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.12.20",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/redux-thunk": "^2.1.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.2",
    "postcss-loader": "^3.0.0",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^1.0.2",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "browserslist": [
    "last 5 versions",
    "ie >= 10"
  ]
}
