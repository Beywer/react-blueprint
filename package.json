{
  "name": "react-blueprint",
  "version": "2.0.0",
  "description": "My react app skeleton",
  "scripts": {
    "start": "cross-env NODE_ENV='development' webpack serve --config configs/webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV='production' webpack --config configs/webpack/webpack.prod.js",
    "lint": "npm-run-all eslint stylelint --continue-on-error",
    "lint:fix": "npm-run-all eslint:fix stylelint:fix prettier --continue-on-error",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier": "prettier src/**/*.{ts,tsx,scss} configs/**/*.js --write",
    "checkPackages": "ts-node --project configs/modules/tsconfig.json configs/modules/checkPackageLock.ts"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "reselect": "4.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@redux-devtools/extension": "3.2.2",
    "@types/fs-extra": "9.0.13",
    "@types/node": "17.0.19",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.22",
    "@types/redux-thunk": "2.1.0",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "autoprefixer": "10.4.2",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.6.0",
    "cssnano": "5.0.17",
    "eslint": "8.9.0",
    "file-loader": "6.2.0",
    "fs-extra": "10.0.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.5.3",
    "node-fetch": "2.6.7",
    "node-sass": "7.0.1",
    "npm-run-all": "4.1.5",
    "postcss-loader": "6.2.1",
    "prettier": "2.5.1",
    "sass-loader": "12.6.0",
    "style-loader": "3.3.1",
    "stylelint": "14.5.1",
    "stylelint-config-standard": "25.0.0",
    "terser-webpack-plugin": "5.3.1",
    "ts-loader": "9.2.6",
    "ts-node": "10.5.0",
    "tslint": "6.1.3",
    "typescript": "4.5.5",
    "webpack": "5.69.1",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  },
  "overrides": {},
  "browserslist": [
    "ie >= 11"
  ]
}
