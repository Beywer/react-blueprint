{
  "name": "react-blueprint",
  "version": "2.0.0",
  "description": "My react app skeleton",
  "scripts": {
    "start": "cross-env NODE_ENV='development' webpack serve --config configs/webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV='production' webpack --config configs/webpack/webpack.prod.js",
    "test": "jest -c configs/jest/jest.config.js --watchAll",
    "lint": "npm-run-all tslint stylelint --continue-on-error",
    "lint:fix": "npm-run-all tslint:fix stylelint:fix prettier --continue-on-error",
    "tslint": "tslint -c configs/tslint/tslint.json src/**/*.ts src/**/*.tsx",
    "tslint:fix": "npm run tslint -- --fix",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier": "prettier src/**/*.{ts,tsx,scss} configs/**/*.js --write"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/redux-thunk": "^2.1.0",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.4",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "browserslist": [
    "ie >= 11"
  ]
}
